    civico:citation "Fleming et al., 2010, Gynecol. Oncol."^^xsd:string ;
    civico:rating "2.0"^^xsd:string ;
    civico:evidenceStatus "accepted"^^xsd:string ;
    civico:evidenceType "Predictive"^^xsd:string ;
