civicv:499
    rdfs:label "ALK FUSIONS" ;
    m2r:evidence civice:945,civice:1175,civice:1187,civice:1198,civice:1199,civice:1200,civice:1201,civice:1202,civice:1240,civice:1243,civice:1264,civice:1272,civice:1273,civice:1279,civice:1282,civice:1334,civice:1335,civice:1419,civice:1577,civice:1691,civice:1783,civice:2952,civice:4835,civice:4858,civice:6042,civice:6043 ;
