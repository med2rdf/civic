civice:1990
    dct:references pbm:27179038 ;
    civico:citation "Pearson et al., 2016, Cancer Discov"^^xsd:string ;
    civico:rating "3.0"^^xsd:string ;
    civico:evidenceStatus "accepted"^^xsd:string ;
